              "body": {
                "mode": "raw",
                "raw": "{\n  \"name\": \"Brand A\",\n  \"code\": \"BR-A\",\n  \"name_en\": \"Brand A EN\",\n  \"contact_person\": \"Alice\",\n  \"phone\": \"0900000000\",\n  \"email\": \"a@brand.com\",\n  \"address\": \"123 Street\",\n  \"website\": \"https://brand.com\",\n  \"description\": \"desc\",\n  \"partnership_date\": \"2025-01-01\",\n  \"contract_number\": \"C-01\",\n  \"commission_rate\": 5,\n  \"sequence\": 10,\n  \"active\": true\n}"
              }
            },
            "event": [
              {
                "listen": "test",
                "script": {
                  "type": "text/javascript",
                  "exec": [
                    "const res = pm.response.json();",
                    "if (res && res.data && res.data.id) {",
                    "  pm.collectionVariables.set('brand_id', String(res.data.id));",
                    "}"
                  ]
                }
              }
